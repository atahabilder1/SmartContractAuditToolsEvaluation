        dividendTracker.excludeFromDividends(address(_uniswapV2Router));

        // exclude accounts from paying fees
        excludeFromFees(owner(), true);
        excludeFromFees(marketingWallet, true);
        excludeFromFees(ownerWalletC, true);
        excludeFromFees(ownerWalletD, true);
        excludeFromFees(address(this), true);

        // Mint PED, send it to devs and track their amount for future dividends
        _mint(_msgSender(), 80000000 * (10**18)); // Future Development Wallet: Does not get any PreyPot reflection
        _mint(marketingWallet, 10000000 * (10**18)); // Marketing Wallet
        try dividendTracker.setBalance(payable(marketingWallet), 10000000 * (10**18)) {} catch {}
        _mint(ownerWalletD, 5000000 * (10**18)); // Dev Wallet 1
        try dividendTracker.setBalance(payable(ownerWalletD), 5000000 * (10**18)) {} catch {}
        _mint(ownerWalletC, 5000000 *(10**18)); // Dev Wallet 2
        try dividendTracker.setBalance(payable(ownerWalletC), 5000000 * (10**18)) {} catch {}

        // Set latest reflection address as reward
        dividendTracker.setNewRewards(_reflectionAddress);
        _nextReflectionAddress = _reflectionAddress;

    }

    // Fallback
    receive() external payable {}

    /**
     * In case the value of PED rises too high (lets hope, NFA) the token amount our contract holds before swapping could be too high and vice versa.
     * Devs can adjust this value. 
     */
    function setTokenAmountBeforeSwap(uint256 _newAmount) external onlyOwner {
        tokenAmountBeforeSwap = _newAmount;
    }

    /**
     * Safe change of new reflection address
     */
    function updateReflectionAddress(address _newAddress) external onlyOwner {
        _nextReflectionAddress = _newAddress;
    }

    /**
     * Fallback change of reflection address for a immediate new reflection address
     */
    function immediateUpdateReflectionAddress(address _newAddress) external onlyOwner {
        _reflectionAddress = _newAddress;
    }

    // Single update for new reflection addrsss of token
    function reflectionUpdate(address _newAddress) private {
       emit ReflectionUpdate(_newAddress);
       _reflectionAddress = address(_newAddress);
       dividendTracker.setNewRewards(_newAddress);
    }

    /**
     * Update PredatorDividendTracker
     * newAddressLocation: Address of PredatorDividendTracker
     * newReflection: Reflection address
     */
    function updateDividendTracker(address newAddressLocation, address newReflection) external onlyOwner {
        emit UpdateDividendTracker(newAddressLocation, address(dividendTracker));
        PredatorDividendTracker newDividendTracker = PredatorDividendTracker(payable(newAddressLocation));
        dividendTracker = newDividendTracker;  
        _reflectionAddress = address(newReflection);
        dividendTracker.setNewRewards(newReflection);
    }

    /** For a decent dividend, holders have to hold a minimum amount of token
     * Initially set: 20,000 tokens
     * Devs can change value based on the current price of PED
     */
    function setMinimumTokenBalanceForDividends(uint256 _tAmount) external onlyOwner {
        dividendTracker.setMinimumTokenBalanceForDividends(_tAmount);
    }

    // Update DEX router address
    function updateUniswapV2Router(address newAddress) external onlyOwner {
        require(newAddress != address(uniswapV2Router), "Predator: The router already has that address");
        emit UpdateUniswapV2Router(newAddress, address(uniswapV2Router));
        uniswapV2Router = IUniswapV2Router02(newAddress);
        address _uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory())
            .createPair(address(this), uniswapV2Router.WETH());
        uniswapV2Pair = _uniswapV2Pair;
    }

    /**
     * Excludes AND includes one account
     */
    function excludeFromFees(address account, bool excluded) public onlyOwner {
        require(_isExcludedFromFees[account] != excluded, "Predator: Account is already excluded");
        _isExcludedFromFees[account] = excluded;
        emit ExcludeFromFees(account, excluded);
    }

    /**
     * Excludes AND includes multiple accounts
     */
    function excludeMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner {
        for(uint256 i = 0; i < accounts.length; i++) {
            _isExcludedFromFees[accounts[i]] = excluded;
        }
        emit ExcludeMultipleAccountsFromFees(accounts, excluded);
    }


    function setAutomatedMarketMakerPair(address pair, bool value) public onlyOwner {
        require(pair != uniswapV2Pair, "Predator: The PancakeSwap pair cannot be modified");
        _setAutomatedMarketMakerPair(pair, value);
    }

    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(automatedMarketMakerPairs[pair] != value, "Predator: Automated market maker pair is already set to that value");
        automatedMarketMakerPairs[pair] = value;
        if(value) {
            dividendTracker.excludeFromDividends(pair);
        }
        emit SetAutomatedMarketMakerPair(pair, value);
    }
    
    /**
     * Blacklist address and remove adress from blacklist
     */
    function blacklistAddress(address account, bool value) external onlyOwner{
        _isBlacklisted[account] = value;
    }

    /**
     * Set high gas amount in case the iterative reward claim costs too much of the initial planned gas
     */
    function updateGasForProcessing(uint256 newValue) external onlyOwner {
        require(newValue >= 200000 && newValue <= 2000000, "Predator: gasForProcessing must be between 200,000 and 2000,000");
        require(newValue != gasForProcessing, "Predator: Cannot update gasForProcessing to same value");
        emit GasForProcessingUpdated(newValue, gasForProcessing);
        gasForProcessing = newValue;
    }

    /**
     * ClaimWait is not a neccessary variable as the distribution is handled through the PreyPot amount.
     * In case of too high volume, a moderative time between distributions CAN be set
     */
    function updateClaimWait(uint256 claimWait) external onlyOwner {
        dividendTracker.updateClaimWait(claimWait);
    }

    /**
     * Get ClaimWait as Unix Time Stamp
     */
    function getClaimWait() external view returns(uint256) {
        return dividendTracker.claimWait();
    }

    /**
     * Get the total amount of dividends distributed
     */
    function getTotalDividendsDistributed() external view returns (uint256) {
        return dividendTracker.totalDividendsDistributed();
    }

    /**
     * Check if account is excluded from fees
     */
    function isExcludedFromFees(address account) public view returns(bool) {
        return _isExcludedFromFees[account];
    }

    /**
     * Check the wthdrawable dividend amount
     */
    function withdrawableDividendOf(address account) public view returns(uint256) {
        return dividendTracker.withdrawableDividendOf(account);
    }

    /**
     * Dividend tokens holding by account
     */
    function dividendTokenBalanceOf(address account) public view returns (uint256) {
        return dividendTracker.balanceOf(account);
    }

    // Exclude from dividends
    function excludeFromDividends(address account) external onlyOwner{
        dividendTracker.excludeFromDividends(account);
    }

    function getAccountDividendsInfo(address account)
        external view returns (
            address,
            int256,
            int256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256) {
        return dividendTracker.getAccount(account);
    }

    function getAccountDividendsInfoAtIndex(uint256 index)
        external view returns (
            address,
            int256,
            int256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256) {
        return dividendTracker.getAccountAtIndex(index);
    }

    /**
     * Starts reflection token distribution.
     * Only devs can initiiate it.
     * This method should not be neccessary as the distribution starts automatically when PreyPot limit is reached.
     */
    function processDividendTracker(uint256 gas) external onlyOwner{
        (uint256 iterations, uint256 claims, uint256 lastProcessedIndex) = dividendTracker.process(gas);
        emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, false, gas, tx.origin);
    }

    /**
     * Claim available rewards in case the automatic distribution won't work for fallback reasons
     */
    function claim() external {
        dividendTracker.processAccount(msg.sender, false);
    }

    function getLastProcessedIndex() external view returns(uint256) {
        return dividendTracker.getLastProcessedIndex();
    }

    function getNumberOfDividendTokenHolders() external view returns(uint256) {
        return dividendTracker.getNumberOfTokenHolders();
    }

    /**
     * Based on the reflection tokn, devs can decide which limit has to be reached for token distribution
     */
    function setPreyPotLimit(uint256 _newLimit) external onlyOwner{
        _preyPotLimit = _newLimit;
        emit SetNewPreyPotLimit(_newLimit);
    }

    function getBuyTax() public view returns(uint256) {
        return _buyTax;
    }

    /**
     * Sets new buyTax.
     * Max is 20% => Security reason: taxes should not take all transferred tokens
     */
    function setBuyTax(uint256 _newBuyTax) external onlyOwner {
        require(_newBuyTax <= 20, "PED: Tax set is too high");
        _buyTax = _newBuyTax;
    }

    function getSellTax() public view returns(uint256) {
        return _sellTax;
    }

    /**
     * Sets new sellTax.
     * Max is 20% => Security reason: taxes should not take all transferred tokens
     */
    function setSellTax(uint256 _newSellTax) external onlyOwner {
        require(_newSellTax <= 20, "PED: Tax set is too high");
        _sellTax = _newSellTax;
    }

    /**
     * Live price analysis for BNB/BUSD
     * Calculates the current price for 1 BNB
     * Returns (in wei) the "dollar" value
     */
    function getPCSValForBNB() public view returns(uint256){
        address[] memory BNBBUSDPair = new address[](2);
        BNBBUSDPair[0] = address(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
        BNBBUSDPair[1] = uniswapV2Router.WETH();
        return uniswapV2Router.getAmountsIn(10**18, BNBBUSDPair)[0];
    }

    /**
     * Get the current value holding of a reflection token => TOKEN/BUSD
     * Price calculation: (TOKEN => BNB => BUSD) * tokenAmount raised by taxes
     * REQUIRES: TOKEN has to have a LP with BNB
     */
    function checkTokenAmountValue(address _token) public view returns(uint256){

        uint256 tokenAmountValue;
        address wethAddress = uniswapV2Router.WETH();
        uint256 BNBPrice = getPCSValForBNB().div(10**18);
        uint256 tokenAmount = IERC20(_token).balanceOf(address(dividendTracker));
        
        if (_token != wethAddress) {
            address[] memory path = new address[](2);
            path[0] = wethAddress;
            path[1] = _token;
            uint256 BNBPricePerToken = uniswapV2Router.getAmountsIn(10**18, path)[0];
            tokenAmountValue = (BNBPrice.mul(BNBPricePerToken)).mul(tokenAmount.div(10**18));
        } else {
            tokenAmountValue = BNBPrice.mul(tokenAmount);
        }
        return tokenAmountValue;
    }

    /**
     * Main function that handles transfers, taxes and decides distribution start
     */
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!_isBlacklisted[from] && !_isBlacklisted[to], 'Blacklisted address');

        if(amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        uint256 contractTokenBalance = balanceOf(address(this));

        // Check if contract holds more PED than threshold value set in the beginning
        bool canSwap = contractTokenBalance >= tokenAmountBeforeSwap;

        if( canSwap &&
            !swapping &&
            !automatedMarketMakerPairs[from] &&
            from != owner() &&
            to != owner()
        ) {
            swapping = true;

            // Get the amount of PED and swap it for reflection token
            uint256 sellTokens = balanceOf(address(this));
            swapAndSendDividends(sellTokens);

            swapping = false;
        }

        // Tax revenue decision
        bool takeFee = !swapping;

        if(_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }

        // Charge taxes for transferring PED
        uint256 currentAmount = amount;
        if(takeFee) {
            uint256 tax = getBuyTax();
            if(automatedMarketMakerPairs[to]){ // Anything that goes back to LP is considered as a Sell
                tax += getSellTax();
            }

            uint256 potFees = (currentAmount.mul(tax.sub(3))).div(100); // PreyPot Fee
            uint256 futDevFees = (currentAmount.mul(3)).div(100); // Future Development Fee
            currentAmount = currentAmount.sub((potFees.add(futDevFees))); // Rest for receiver

            super._transfer(from, owner(), futDevFees);
            super._transfer(from, to, currentAmount);
            super._transfer(from, address(this), potFees);
        } else {
            super._transfer(from, to, currentAmount); // Send all if excluded from fees
        }

        // Track new PED amount holding by the accounts
        try dividendTracker.setBalance(payable(from), balanceOf(from)) {} catch {}
        try dividendTracker.setBalance(payable(to), balanceOf(to)) {} catch {}

        // If not in a swapping state, look if PreyPot limit is reached and start distribution
        if(!swapping) {
            // Check if value of current amount of reflection tokens is hgher that the PreyPot limit
            if (checkTokenAmountValue(_reflectionAddress) >= _preyPotLimit.mul(10**18)) {

                uint256 gas = gasForProcessing;

                try dividendTracker.process(gas) returns (uint256 iterations, uint256 claims, uint256 lastProcessedIndex) {
                    emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, true, gas, tx.origin);
                }
                catch {

                }
                // Safe update new reflection address after token distribution
                if (_nextReflectionAddress != _reflectionAddress) {
                reflectionUpdate(_nextReflectionAddress);
            }
        }
    }
    }

    /**
     * Swap PED for reflection token
     */
    function swapTokensForReflectionToken(uint256 tokenAmount) private {

        address weth = uniswapV2Router.WETH();

        // In case the reflection token is WBNB:
        // Swap PED for BNB and wrap it
        if (_reflectionAddress == weth) {

            swapTokensForBNB(tokenAmount);
            IERC20EXT(weth).deposit{value : address(this).balance}();

        } else {

        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = weth;
        path[2] = _reflectionAddress;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
        }

    }        

    /**
     * Swap token for BNB
     */
    function swapTokensForBNB(uint256 tokenAmount) private {

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );

    }

    /**
     * Swap PED for reflection token.
     * Send reflection token to dividendTracker as that is responsible for the final distribution
     */
    function swapAndSendDividends(uint256 tokens) private {
        swapTokensForReflectionToken(tokens);
        uint256 dividends = IERC20(_reflectionAddress).balanceOf(address(this));

        bool success = IERC20(_reflectionAddress).transfer(address(dividendTracker), dividends);
        if (success) {
            dividendTracker.distributeTokenDividends(dividends);
            emit SendDividends(tokens, dividends);
        }
    }
}