    IUniswapV2Router02 public uniswapV2Router;
    address public  uniswapV2Pair;

    bool private swapping; // Swapping state variable. Must not be deleted

    PredatorDividendTracker public dividendTracker;

    address public deadWallet = 0x000000000000000000000000000000000000dEaD; // Burning address

    address public _reflectionAddress = address(0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c); // Current reflection token address - Initial: WBNB

    address public _nextReflectionAddress; // New reflection address that only takes use after a distribution to not interfere with the state of dividendtracker

    uint256 public _buyTax =   10; // Initial buy tax : ALWAYS 3% go to Owner, rest Prey Pot
    uint256 public _sellTax = 10; // ADDITIONAL tax to buy

    uint256 public tokenAmountBeforeSwap = 20000 * (10**18); // Amount of PED holding by contract before swapping (value changeable)

    uint256 public _preyPotLimit = 10000; // Limit that has to be reached (or >) Prey Pot starts distributing

    mapping(address => bool) public _isBlacklisted; // Blacklist if neccessary

    // DEV wallets
    address public marketingWallet = 0x94515b6e0610B7e86939bC1C770D9d5B3396fD68;
    address private ownerWalletD = 0x359a3FE471ECe04e76e2062E0421Db3A840977fE;
    address private ownerWalletC = 0xE27B7626631884a0E4d5E7fA82Be462a8357e09C;

    uint256 public gasForProcessing = 699000; // Initial gas amount to be used

    mapping (address => bool) private _isExcludedFromFees; // Fee exclusion and inclusion mapping

    mapping (address => bool) public automatedMarketMakerPairs; // LP mapping

    /**
     * EVENTS
     */
    event UpdateDividendTracker(address indexed newAddress, address indexed oldAddress);
    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);
    event ExcludeFromFees(address indexed account, bool isExcluded);
    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);
    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);
    event ReflectionUpdate(address indexed newr);
    event GasForProcessingUpdated(uint256 indexed newValue, uint256 indexed oldValue);
    event SendDividends(
        uint256 amount,
        uint256 dividends
    );
    event ProcessedDividendTracker(
        uint256 iterations,
        uint256 claims,
        uint256 lastProcessedIndex,
        bool indexed automatic,
        uint256 gas,
        address indexed processor
    );
    event SetNewPreyPotLimit(uint256 limitAmount);

