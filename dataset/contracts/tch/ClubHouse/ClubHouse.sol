    address public _operationsWallet = 0x8ef2d33835b3F12135AEFE9063B0130f61C52509;
    address public _stakingWallet = 0x4A08b1B4A82Acb85a070B9c6bC9c4a84a519F2a1;
    address public constant router = 0x10ED43C718714eb63d5aA57B78B54704E256024E;   
    uint256 public BNBThreshold = 2 * 10 ** 17; //BNB threshold to trigger distribution
  

    uint256 public walletMax = 15 * 10**5 * (10**9); //max wallet 1.5M

    //fees

    uint256 public BuyLiquidityFee = 2;
    uint256 public BuyMarketingFee = 4;
    uint256 public buyTotalFees;

    uint256 public sellLiquidityFee = 2;
    uint256 public SellMarketingFee = 5;
    uint256 public sellTotalFees;

    //mappings

    mapping(address=>bool) public automatedMarketMaker;
    mapping(address=>bool) public blacklisted;
    mapping (address => bool) public isExcludedFromFees;
    mapping (address => bool) public excludedFromMaxWallet;

    bool private swapping;
    bool private isEnabledTrading;

    //events
    
    event ContractChanged(uint256 indexed value);
    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);
    event SwapAndLiquify(uint256 tokensSwapped, uint256 ethReceived, uint256 tokensIntoLiqudity);
    event distributedProtocolBNB(uint256 BNBDistributed);

    constructor() ERC20("Club House", "TCH") {

        buyTotalFees = BuyLiquidityFee.add(BuyMarketingFee);
        sellTotalFees = sellLiquidityFee.add(SellMarketingFee);

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(router); 

         // Create a uniswap pair for this new token and register the AMM
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
        automatedMarketMaker[_uniswapV2Pair] = true;

        // exclude from paying fees or having max transaction amount

        setUserFeesState(owner(), true);
        setUserFeesState(address(this), true);
        setUserFeesState(_projectWallet, true);
        setUserFeesState(_operationsWallet, true);
        setUserFeesState(_stakingWallet, true);

        //mint 100M

        _mint(owner(), 100 * 10**6 * (10**9));  
    }

    receive() external payable {}

    function updatebuyFees(uint256 newBuyLiqFee, uint256 newBuyMarketingFee) public onlyOwner {
        buyTotalFees = newBuyLiqFee.add(newBuyMarketingFee);
        require(buyTotalFees<=20 && buyTotalFees>0); //max fees 20%
        BuyLiquidityFee = newBuyLiqFee;
        BuyMarketingFee = newBuyMarketingFee;
    }

    function updatesellFees(uint256 newsellLiqFee, uint256 newSellMarketingFee) public onlyOwner {
        sellTotalFees = newsellLiqFee.add(newSellMarketingFee);
        require(sellTotalFees<=20 && sellTotalFees>0); //max fees 20%
        sellLiquidityFee = newsellLiqFee;
        SellMarketingFee = newSellMarketingFee;
    }

    function updateBNBThreshold(uint256 newThreshold) public onlyOwner{
        //start from first BNB decimal
        require(newThreshold> 0 && newThreshold<=10); //max 1BNB
        BNBThreshold = newThreshold * 10 ** 17;
    }

    function setStakingWallet(address payable wallet) public onlyOwner {
        require(wallet != address(0));
        require(wallet != _stakingWallet);
        _stakingWallet = wallet;
    }

    function setProjectWallet(address payable wallet) public onlyOwner {
        require(wallet != address(0));
        require(wallet != _projectWallet);
        _projectWallet = wallet;
    }

    function setOperationWallet(address payable wallet) public onlyOwner {
        require(wallet != address(0));
        require(wallet != _operationsWallet);
        _operationsWallet = wallet;
    }

    function updateUniswapV2Router(address newAddress) public onlyOwner {
        require(newAddress != address(0), "The router already has that address");
        require(newAddress != address(uniswapV2Router), "The router already has that address");
        emit UpdateUniswapV2Router(newAddress, address(uniswapV2Router));
        uniswapV2Router = IUniswapV2Router02(newAddress);
    }

    function setAutomatedMarketMaker(address _amm, bool isAmm) public onlyOwner{
        require(_amm != address(0) && _amm != address(this));
        automatedMarketMaker[_amm] = isAmm;
    }

    function setUserFeesState(address account, bool excluded) public onlyOwner {
        require(isExcludedFromFees[account] != excluded, "Account is already in the selected state");
        isExcludedFromFees[account] = excluded;
    }

    function setUserMaxWalletState(address account, bool excluded) public onlyOwner {
        require(excludedFromMaxWallet[account] != excluded, "Account is already in the selected state");
        excludedFromMaxWallet[account] = excluded;
    }

    function changeWalletLimit(uint256 newLimit) external onlyOwner {
        require(newLimit*10**9 >= 1*(10**6)*(10**9)); //never go below 1M
        walletMax  = newLimit * 10 ** 9;
    } 

    function blacklistAddress(address account, bool blacklist) public onlyOwner {
        blacklisted[account] = blacklist;
    }

    function enableTrading() public onlyOwner{
        isEnabledTrading = true; //can't be set back to false
    }

    function _transfer(address from, address to, uint256 amount ) internal override {

        require(to != address(0), "ERC20: transfer to the zero address");
        require(from != address(0), "ERC20: transfer from the zero address");
        require(!blacklisted[from] && !blacklisted[to], "Transfer made by blacklisted address");

        if(amount == 0) {
            super._transfer(from, to, 0);
            return;
        }


        // check max wallet

        if(!isExcludedFromFees[to] && !automatedMarketMaker[to] && !excludedFromMaxWallet[to]){
            require(balanceOf(to).add(amount) <= walletMax, "Wallet limit reached");
        }


        if(!swapping && !automatedMarketMaker[from] && from != owner() && to!= owner()) {
            uint bal = balanceOf(address(this));
            if(bal>0){
                swapping = true;
                swapBack(bal);}
                swapping=false;
        }

        bool takeFee = !swapping;

        if(from == uniswapV2Pair){require (isEnabledTrading);} //initially only one pool (uniswapV2Pair), this is to block sniping bots.

        // if any account is excluded from fees or it is a transfer without the amm being involved remove fees

        if(isExcludedFromFees[from] || isExcludedFromFees[to] || (!automatedMarketMaker[from] && !automatedMarketMaker[to])) {
            takeFee = false;
        }

        if(takeFee) {

            //Sell
            if(automatedMarketMaker[to]) {
                uint256 sellFees = amount.mul(sellTotalFees).div(100);
                amount = amount.sub(sellFees);
                super._transfer(from, address(this), sellFees);
            }
            //Buy
            else {
                uint256 buyFees = amount.mul(buyTotalFees).div(100);
                amount = amount.sub(buyFees);
                super._transfer(from, address(this), buyFees);
            }
        }
        super._transfer(from, to, amount);
    }

    function swapBack(uint256 contractBalance) internal  {

        uint256 tokensToLiquify = contractBalance.mul(sellLiquidityFee).div(sellTotalFees).div(2);
        uint256 amountToSwap = contractBalance.sub(tokensToLiquify);

        //register BNB balance before the swap

        uint256 balanceBefore = address(this).balance;
        
        //make the swap

        swapTokensForEth(amountToSwap);
        
        //register amount of received BNB

        uint256 receivedBNB = address(this).balance.sub(balanceBefore);
        uint256 totalBNBFee = sellTotalFees.sub(sellLiquidityFee.div(2));

        //add liquidity

        uint256 amountBNBLiquidity = receivedBNB.mul(sellLiquidityFee).div(totalBNBFee).div(2);
        addLiquidity(tokensToLiquify, amountBNBLiquidity);

        //distribute BNB

        uint256 remainingBNB = address(this).balance;
        if (remainingBNB > BNBThreshold){distributeProtocolBNB(remainingBNB);}
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            _projectWallet,
            block.timestamp
        );
        emit SwapAndLiquify(tokenAmount, ethAmount, tokenAmount);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
   
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );    
    }

    function distributeProtocolBNB(uint256 balance) private {
        (bool successStaking,) = address(_stakingWallet).call{value: balance.mul(20).div(100)}("");
        (bool successOperations,) = address(_operationsWallet).call{value: balance.mul(30).div(100)}("");
        (bool successProject,) = address(_projectWallet).call{value: address(this).balance}("");
        if(successStaking && successOperations && successProject){emit distributedProtocolBNB(balance);}
    }

    function withdrawStuckBNB() external onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function withdrawTokens(address token) external onlyOwner {
        require(token!= address(this)); // can not withdraw CH
        IERC20 tokenContract = IERC20(token);
        tokenContract.transfer(msg.sender, tokenContract.balanceOf(address(this)));
    }
}